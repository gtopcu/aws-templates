{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Sid": "policy01",
            "Effect": "Allow",
            "Action": [
                "s3:*"
            ],
            "Resource": "*"
        }
    ]
}

{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Principal": "*",
            "Action": "s3:*",
            "Resource": "arn:aws:s3:::{bucket_name}"
        }
    ]
}

// "Resource": [
// 	"arn:aws:s3:::dev-*/*",
// 	"arn:aws:s3:::dev-*"
// ]

{
	"Version": "2012-10-17",
	"Statement": [
	  {
		"Effect": "Allow",
		"Action": "s3:PutObject",
		"Resource": "arn:aws:s3:::BucketName/*"
	  }
	]
  }

//Deny without SSL:
{
	"Id": "BlockNonSSLAccess",
	"Version": "2012-10-17",
	"Statement": [
	  {
		"Sid": "AllowSSLRequestsOnly",
		"Action": "s3:*",
		"Effect": "Deny",
		"Resource": [
			"arn:aws:s3:::config-bucket-001134395538",
			"arn:aws:s3:::config-bucket-001134395538/*"
		],
		"Condition": {
		  "Bool": {
			"aws:SecureTransport": "false"
		  }
		},
		"Principal": "*"
	  }
	]
}